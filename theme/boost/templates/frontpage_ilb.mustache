                        <li><a href="http://www12.senado.leg.br/ecidadania/">e-Cidadania</a></li>
                    </ul>
                </div>
            </div>
            <div class="u-sizeFit"><a href="http://www.senado.gov.br/"><img src="http://www.senado.gov.br/inc/essencial/img/senado.svg" alt="Senado Federal" width="170" height="23"></a></div>
            <div class="u-sizeFill text-center">
                <div class="hidden-xs"><a href="http://www12.senado.gov.br/institucional/falecomosenado" class="flat">Fale com o Senado</a></div>
            </div>
        </div>
    </div>
</div>

<div id="page-wrapper">
    {{{ output.standard_top_of_body_html }}}

    <div id="page" class="container-fluid">
        <div id="page-content" class="row">
            <div id="region-main-box" class="col-xs-12">
                <section id="region-main">
                    <div class="card card-block">
                   {{{ output.course_content_header }}} 
                   {{{ output.main_content }}} 
                   {{{ output.course_content_footer }}}
                    </div>
                </section>
            </div>
        </div>
    </div>
</div>
